
<%- include('./partials/header'); -%>

<h1>Home</h1>
<p> <%= homeStartingContent %> </p>


<% allPosts.forEach((post) => { %>
  <h1> <%= post.name %> </h1>
  <p>
    <%= post.body.slice(0, 100) + '...' %>
    <a href= "/posts/<%= post.nameModified %>"> Read More</a>
  </p>

<% }); %>

<%- include('./partials/footer'); -%>
